<?xml version="1.0" encoding="utf-8" ?>

<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="Domain"
                   namespace="Domain">

  <class name="Coureur" table="coureur">
    <id name="ID" column="id" type="int">
      <generator class="native"></generator>
    </id>
    <property name="Nom" column="nom_sponsor" not-null="true"/>

    <bag name="Sponsorises" cascade="all" lazy="true">
      <key column="sponsor_id"/>
      <one-to-many class="Sponsorise"/>
    </bag>

  </class>

</hibernate-mapping>
